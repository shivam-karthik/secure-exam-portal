    </nav>
  </div>
  </section>

    
  <section id="routes">
    <div class="container">

        <div class="row">

           <div class="col-lg-12">
            <% if (designation == "Teacher"){ %>

              <% if(typeof results[0] == undefined){ %>
                <div class="alert alert-dismissible alert-danger container">
                  <button type="button" class="close" data-dismiss="alert">&times;</button>
                  <h4 class="alert-heading">No Result to Show</h4>  
                </div>
              <%}%>
          
              
              <h1>Results By Quiz Name</h1>
          
              <% if(typeof results != 'undefined'){ %>
              
                <div class="container" style="text-align: center;">
                    
                  <table class="table table-sm table-striped table-dark">
                    <thead>
                      <tr class="bg-primary">
                        <th scope="col">UserId</th>
                        <th scope="col">Score</th>
                      </tr>
                    </thead>
                    <tbody>
                        <%  results.forEach(function(result){ %>
                        <tr class="bg-info">
                            <td ><%= result.UserId %></td>
                            <td ><%= result.Score %></td>

                        </tr>
                        <% }) %>
                    </tbody>
                  </table>
          

               <% } %>

              <% } %>
      
           </div>




           <div class="col-lg-12">
            <% if (designation == "Student"){ %>

              <% if(typeof quizes == undefined){ %>
                <div class="alert alert-dismissible alert-danger container">
                  <button type="button" class="close" data-dismiss="alert">&times;</button>
                  <h4 class="alert-heading">No Result to Show</h4>  
                </div>
              <%}%>
          
              
              <h1>Your Result</h1>
          
              <% if(typeof quizes != 'undefined'){ %>
              
                <p style="color:white;">You Scored <%= quizes.Score %>/<%= totalmarks %> in <%= quizname %></p>   

              <% } %>

            <% } %>
           </div>
        </div>
      </div>
<!-- bootstrap script tag -->
   <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
   <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>